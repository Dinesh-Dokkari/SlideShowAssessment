@model SlidesShow.Models.createslide

<br/>
<h4>Create Slide</h4>
<hr />

        <form enctype="multipart/form-data" asp-action="Create">
            <table>
            <div class="form-group">
            <input type="hidden" asp-for="PropertyId" value="@ViewData["propId"]" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Image" class="control-label"></label>
                <input asp-for="Image" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Duration" class="control-label"></label>
                <input asp-for="Duration" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="BackgroundColor" class="control-label"></label>
                <input asp-for="BackgroundColor" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Footer" class="control-label"></label>
                <input asp-for="Footer" class="form-control" />
            </div>
            <br />
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
    </table>
</form>
        


